{
  "name": "co-bearn-middleware",
  "version": "0.1.0",
  "private": true,
  "description": "Middleware for co-bearn.org",
  "keywords": [
    "semapps",
    "semantic"
  ],
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "devwithlink": "make link & sleep 5 && npm run dev",
    "start": "moleculer-runner --instances=max services",
    "link-all": "npm link @semapps/activitypub && npm link @semapps/ldp && npm link @semapps/webhooks"
  },
  "dependencies": {
    "@semapps/backup": "0.3.17",
    "@semapps/fuseki-admin": "0.3.17",
    "@semapps/inference": "0.3.17",
    "@semapps/jsonld": "^0.3.17",
    "@semapps/ldp": "0.3.17",
    "@semapps/mime-types": "0.3.17",
    "@semapps/sparql-endpoint": "0.3.17",
    "@semapps/triplestore": "0.3.17",
    "@semapps/webid": "^0.3.17",
    "dotenv-flow": "^3.2.0",
    "handlebars": "^4.7.7",
    "ioredis": "^4.17.3",
    "moleculer": "^0.14.18",
    "moleculer-mail": "^1.2.5",
    "moleculer-repl": "^0.6.6",
    "moleculer-web": "^0.10.3",
    "url-join": "^4.0.1"
  }
}
